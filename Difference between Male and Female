//Difference between male and female studenrs

function genderBlocking()
//Accept JSON file and find out how many more males or females there are than the other gender. With this difference we can find figure out how many rooms to change.
{
    let data = ['data\blueprint.xls'];
    let floor1Male = []
    let floor1Female = []
    let floor2Male =[]
    let floor2Female = []
    let floor3Male =[]
    let floor3Female = []
    let floor4Male =[]
    let floor4Female = []
    let floor5Male =[]
    let floor5Female = []
    
    for (let i = 0; i < data.length; i++) 
    {
        data = [i]
        if (data[i].Gender == 'Male') {
            if (data[i].Floor_Type == 'Floor1') 
            {
                floor1Male.push[data[i]]
            }
            else 
            {
                floor1Female.push(data[i])
            }
            else 
            {
                if (data[i].Floor_Type == 'Floor1') 
                {
                    floor1Female.push(data[i])
            }
            }
            else 
            {
                floor1Male.push(data.[i])
            }
            
    }
    for (let i = 0; i < data.length; i++) 
    {
        data = [i]
        if (data[i].Gender == 'Male') {
            if (data[i].Floor_Type == 'Floor2') 
            {
                floor2Male.push[data[i]]
            }
            else 
            {
                floor2Female.push(data[i])
            }
            else 
            {
                if (data[i].Floor_Type == 'Floor2') 
                {
                    floor2Female.push(data[i])
            }
            }
            else 
            {
                floor2Male.push(data.[i])
            }
            
            }
            for (let i = 0; i < data.length; i++) 
    {
        data = [i]
        if (data[i].Gender == 'Male') {
            if (data[i].Floor_Type == 'Floor3') 
            {
                floor3Male.push[data[i]]
            }
            else 
            {
                floor3Female.push(data[i])
            }
            else 
            {
                if (data[i].Floor_Type == 'Floor3') 
                {
                    floor3Female.push(data[i])
            }
            }
            else 
            {
                floor3Male.push(data.[i])
            }
            
            }
            for (let i = 0; i < data.length; i++) 
    {
        data = [i]
        if (data[i].Gender == 'Male') {
            if (data[i].Floor_Type == 'Floor4') 
            {
                floor4Male.push[data[i]]
            }
            else 
            {
                floor4Female.push(data[i])
            }
            else 
            {
                if (data[i].Floor_Type == 'Floor4') 
                {
                    floor4Female.push(data[i])
            }
            }
            else 
            {
                floor4Male.push(data.[i])
            }
            
            }
            for (let i = 0; i < data.length; i++) 
    {
        data = [i]
        if (data[i].Gender == 'Male') {
            if (data[i].Floor_Type == 'Floor5') 
            {
                floor5Male.push[data[i]]
            }
            else 
            {
                floor5Female.push(data[i])
            }
            else 
            {
                if (data[i].Floor_Type == 'Floor5') 
                {
                    floor5Female.push(data[i])
            }
            }
            else 
            {
                floor5Male.push(data.[i])
            }
            
            }
            
           
        }
    }
}

function differenceofGender(floorplanJSON, totalMale, totalFemale)
    {
        if (totalMale > totalFemale)
        {
            diff = totalMale - totalFemale // Calculate the difference between male over females
            return true
        }
        else if  (totalFemale > totalMale)
        {
            diff = totalFemale - totalMale // Calculate the difference between females over males 
            return true
        }
        
        return false //if there is the same amount of females and males in each floor 

    }
